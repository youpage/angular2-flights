{
  "name": "flight_search",
  "version": "0.0.2",
  "description": "RC6 and Unit Testing",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/youpage/angular2-flights.git"
  },
  "scripts": {
    "postinstall": "typings install --ambient",
    "clean": "rimraf built/",
    "copy": "cpx \"src/**/*.{html,js,cs}\" built/",
    "copy:w": "cpx \"src/**/*.{html,js,cs}\" built/ --watch",
    "build": "tsc && npm run copy",
    "watch": "tsc --watch",
    "tsc": "tsc",
    "serve": "http-server -p 9090 -c-1",
    "test": "karma start karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "cpx": "^1.4.0",
    "http-server": "^0.8.5",
    "jasmine": "2.3.2",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "rimraf": "^2.4.3",
    "typescript": "1.8.10",
    "typings": "^0.6.6"
  }
}
